<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>io.mrinal.upgrade</string>

  <key>ProgramArguments</key>
  <array>
    <string>/Users/m/.system/bin/upgrade</string>
  </array>

  <key>Nice</key>
  <integer>1</integer>

  <key>StartInterval</key>
  <integer>86400</integer>

  <key>RunAtLoad</key>
  <true/>

  <key>StandardErrorPath</key>
  <string>/tmp/upgrade.err.log</string>

  <key>StandardOutPath</key>
  <string>/tmp/upgrade.out.log</string>
</dict>
</plist>
